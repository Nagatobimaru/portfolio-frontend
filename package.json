{
  "name": "portfolio-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prettier": "cross-env prettier --write --ignore-path .gitignore --cache --cache-strategy content",
    "eslint": "cross-env eslint --ignore-path .gitignore --cache --cache-location node_modules/.cache/eslint/ --cache-strategy content",
    "stylelint": "cross-env stylelint --ignore-path .gitignore --cache --cache-location node_modules/.cache/stylelint/",
    "format": "yarn prettier --list-different '.'",
    "lint": "yarn lint:ts-compile && yarn lint:js && yarn lint:vue && yarn lint:css",
    "lint:ts-compile": "yarn vue-tsc --project ./tsconfig.json --noEmit",
    "lint:js": "yarn eslint '**/*.{js,ts}'",
    "lint:vue": "yarn eslint '**/*.vue' && yarn stylelint '**/*.vue'",
    "lint:css": "yarn stylelint '**/*.{css,scss}'",
    "lint:fix": "yarn lint:fix:js && yarn lint:fix:vue && yarn lint:fix:css && yarn format",
    "lint:fix:js": "yarn lint:js --fix",
    "lint:fix:vue": "yarn eslint '**/*.vue' --fix && yarn stylelint '**/*.vue' --fix",
    "lint:fix:css": "yarn lint:css --fix"
  },
  "lint-staged": {
    "*.{js,ts}": "yarn lint:fix:js",
    "*.{vue}": "yarn lint:fix:vue",
    "*.{css,scss}": "yarn lint:fix:css",
    "*": "yarn format"
  },
  "devDependencies": {
    "@kevinmarrec/nuxt-pwa": "^0.17.0",
    "@modyfi/vite-plugin-yaml": "^1.0.4",
    "@nuxt/devtools": "latest",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.18.1",
    "eslint-plugin-vue-scoped-css": "^2.5.1",
    "eslint-plugin-vuejs-accessibility": "^2.2.0",
    "eslint-plugin-vuetify": "^2.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "nuxt": "^3.8.2",
    "prettier": "^3.1.0",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "typescript": "^5.3.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-css-export": "^2.0.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "vite": "^5.0.2"
  },
  "engines": {
    "node": ">= 18"
  }
}
